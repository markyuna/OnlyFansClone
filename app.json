{
  "expo": {
    "scheme": "acme",
    "splash": {
      "image": "./assets/images/loading.gif",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-router"
    ],
    "name": "OnlyFansClone",
    "slug": "OnlyFansClone",
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.markyuna.OnlyFansClone"
    }
  }
}
